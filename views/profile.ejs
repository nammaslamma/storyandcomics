<!DOCTYPE html>
<html>
<head>
    <title><%= user.username %>'s Profile</title>
</head>
<body>
    <h1><%= user.username %>'s Profile</h1>
    <p><%= user.bio %></p>

    <h2>Stories by <%= user.username %>:</h2>
    <ul>
        <% stories.forEach(story => { %>
            <li>
                <h3><%= story.title %></h3>
                <p><%= story.content.substring(0, 100) %>... <a href="#">Read more</a></p>
            </li>
        <% }) %>
    </ul>
</body>
</html>
